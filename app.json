{
  "expo": {
    "name": "LiquorBarcodeApp",
    "slug": "liquor-barcode-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.tl.liquorbarcodeapp"
    },
    "web": {
      "favicon": "./favicon.png"
    }
  }
}
